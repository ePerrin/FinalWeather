# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'FinalWeather' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for FinalWeather
  
  #AFNetworking - HTTP utilities to help using of ios http objects
  #   https://github.com/AFNetworking/AFNetworking
  pod 'AFNetworking', '~> 3.1.0'
  
  # Swifty Json - Serialization/Deserialization of Json objects
  #   https://github.com/SwiftyJSON/SwiftyJSON
  pod 'SwiftyJSON', '~> 3.0.0'

  target 'FinalWeatherTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'FinalWeatherUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
